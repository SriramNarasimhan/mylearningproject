<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceSuperType="/apps/dotcom/components/page/page/cq:dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container"
                rel="cq-siteadmin-admin-properties-tabs">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <profileinformation
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Profile Information"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <firstname
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the first name"
                                        fieldLabel="First Name"
                                        name="./adv_firstname"
                                        required="{Boolean}true"/>
                                    <lastname
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the last name"
                                        fieldLabel="Last Name"
                                        name="./adv_lastname"/>
                                    <emailaddress
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the email address"
                                        fieldLabel="Email Address"
                                        name="./adv_emailaddress"/>
                                    <title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the advisor title1"
                                        fieldLabel="Title1"
                                        name="./adv_title1"/>
                                    <title2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the advisor title2"
                                        fieldLabel="Title2"
                                        name="./adv_title2"/>
                                    <designationcodes
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                        allowCreate="{Boolean}false"
                                        cq-msm-lockable="cq:tags"
                                        fieldDescription="Select the designation codes"
                                        fieldLabel="Designation Codes"
                                        forceSelection="{Boolean}true"
                                        metaType="tags"
                                        multiple="{Boolean}true"
                                        name="./adv_designationcodes"
                                        rootPath="/etc/tags/designationcodes"/>
                                    <nmls
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the nmls number"
                                        fieldLabel="NMLS"
                                        name="./adv_nmls"/>
                                    <cities
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        disabled="{Boolean}true"
                                        fieldLabel="Cities"
                                        name="./adv_cities"/>
                                    <state
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                        allowCreate="{Boolean}false"
                                        cq-msm-lockable="cq:tags"
                                        disabled="{Boolean}true"
                                        fieldDescription="Select the state name"
                                        fieldLabel="State"
                                        forceSelection="{Boolean}true"
                                        metaType="tags"
                                        multiple="{Boolean}true"
                                        name="./adv_state"
                                        rootPath="/etc/tags/states"/>
                                    <phone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the phone number"
                                        fieldLabel="Phone"
                                        name="./adv_phone"/>
                                    <cell
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the cell number"
                                        fieldLabel="Cell"
                                        name="./adv_cell"/>
                                    <fax
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the fax number"
                                        fieldLabel="Fax"
                                        name="./adv_fax"/>
                                    <specialties
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                        allowCreate="{Boolean}false"
                                        cq-msm-lockable="cq:tags"
                                        fieldDescription="Select the specialty"
                                        fieldLabel="Specialty"
                                        forceSelection="{Boolean}true"
                                        metaType="tags"
                                        multiple="{Boolean}false"
                                        name="./adv_specialty"
                                        rootPath="/etc/tags/advisor-specialty"/>
                                    <facebook
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the facebook url"
                                        fieldLabel="Facebook"
                                        name="./adv_facebook"/>
                                    <twitter
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the twitter url"
                                        fieldLabel="Twitter"
                                        name="./adv_twitter"/>
                                    <linkedin
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the linkedin url"
                                        fieldLabel="LinkedIn"
                                        name="./adv_linkedin"/>
                                    <youtube
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the youtube url"
                                        fieldLabel="Youtube"
                                        name="./adv_youtube"/>
                                    <googleplus
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the googleplus url"
                                        fieldLabel="Google Plus"
                                        name="./adv_googleplus"/>
                                </items>
                            </column>
                        </items>
                    </profileinformation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
