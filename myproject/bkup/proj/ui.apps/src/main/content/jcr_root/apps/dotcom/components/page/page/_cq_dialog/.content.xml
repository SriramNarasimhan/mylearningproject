<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container"
                rel="cq-siteadmin-admin-properties-tabs">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items
                                    jcr:primaryType="nt:unstructured"
                                    sling:hideChildren="vanityurl">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Title and Tags"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagename
                                                cq:hideOnEdit="{Boolean}true"
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Name"
                                                name="pageName"
                                                validation="admin.pagename"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                cq-msm-lockable="jcr:title"
                                                fieldLabel="Title"
                                                name="./jcr:title"
                                                renderReadOnly="{Boolean}true"
                                                required="{Boolean}true"/>
                                            <msmConfigEndpoint
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                class="msm-config-endpoint"
                                                value="${empty param.item ? requestPathInfo.suffix : param.item }"/>
                                            <tags
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/common/tagspicker"
                                                allowBulkEdit="{Boolean}true"
                                                allowCreate="{Boolean}true"
                                                cq-msm-lockable="cq:tags"
                                                fieldLabel="Tags"
                                                name="./cq:tags"
                                                renderReadOnly="{Boolean}true"/>
                                            <hideinnav
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="hideInNav"
                                                name="./hideInNav"
                                                renderReadOnly="{Boolean}true"
                                                text="Hide in Navigation"
                                                value="true"/>
                                            <charset
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="_charset_"
                                                value="utf-8"/>
                                        </items>
                                    </title>
                                    <moretitles
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="More Titles and Description"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagetitle
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="pageTitle"
                                                fieldLabel="Page Title"
                                                name="./pageTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <navigationtitle
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="navTitle"
                                                fieldLabel="Navigation Title"
                                                name="./navTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <subtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="subtitle"
                                                fieldLabel="Subtitle"
                                                name="./subtitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="jcr:description"
                                                fieldLabel="Description"
                                                name="./jcr:description"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </moretitles>
                                    <onofftime
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="On/Off Time"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ondate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="onTime"
                                                displayedFormat="MMMM DD, YYYY hh:mm a"
                                                displayTimezoneMessage="{Boolean}true"
                                                fieldLabel="On Time"
                                                name="./onTime"
                                                renderReadOnly="{Boolean}true"
                                                type="datetime"/>
                                            <offdate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                allowBulkEdit="{Boolean}true"
                                                beforeSelector="coral-datepicker[name=&quot;./onTime&quot;]"
                                                cq-msm-lockable="offTime"
                                                displayedFormat="MMMM DD, YYYY hh:mm a"
                                                displayTimezoneMessage="{Boolean}true"
                                                fieldLabel="Off Time"
                                                name="./offTime"
                                                renderReadOnly="{Boolean}true"
                                                type="datetime"/>
                                        </items>
                                    </onofftime>
                                    <vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Vanity URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="sling:vanityPath"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"/>
                                            </vanitypath>
                                            <redirectVanityURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="./sling:redirect"
                                                name="./sling:redirect"
                                                renderReadOnly="{Boolean}true"
                                                text="Redirect Vanity URL"
                                                value="true"/>
                                        </items>
                                    </vanityurl>
                                    <st-vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="SunTrust Vanity URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                fieldDescription="Configure Vanity URL"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    different-values-group="vanity"
                                                    emptyText="/vanity-url.html"
                                                    errormsg="Vanity URL already exists"
                                                    errormsgfield="Duplicate URL"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"
                                                    validation="different-values-validation,unique-vanityurl"/>
                                            </vanitypath>
                                        </items>
                                    </st-vanityurl>
                                    <st-redirects
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="SunTrust Redirects"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <redirectpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                fieldDescription="Configure Redirect"
                                                fieldLabel="Redirects"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    different-values-group="vanity"
                                                    emptyText="/redirects"
                                                    errormsg="Redirect URL already exists"
                                                    errormsgfield="Duplicate Redirect URL"
                                                    name="./st-redirects"
                                                    required="{Boolean}true"
                                                    validation="different-values-validation,unique-vanityurl"/>
                                            </redirectpath>
                                        </items>
                                    </st-redirects>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <thumbnail
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Thumbnail"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="cq:WidgetCollection">
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Image"
                                        sling:resourceType="granite/ui/components/foundation/section">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                            margin="{Boolean}false"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <url
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Sets the thumbnail image"
                                                        fieldLabel="Thumbnail"
                                                        name="./image/fileReference"
                                                        rootPath="/content/dam"/>
                                                    <alttext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                        fieldDescription="Alt Text"
                                                        fieldLabel="Alt Text"
                                                        maxlength="{Long}100"
                                                        name="./image/alttext"/>
                                                </items>
                                            </column>
                                        </items>
                                    </image>
                                </items>
                            </column>
                        </items>
                    </thumbnail>
                    <section
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Site Config"
                        sling:orderAfter="basic"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <items jcr:primaryType="nt:unstructured">
                            <configs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <signonpath
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldDescription="This field is used to configure Sign On Page Path"
                                        fieldLabel="Sign On Page Path"
                                        name="./signonpage"
                                        rootPath="/content"/>
                                    <errorpage
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldDescription="Error pages for this Content Tree"
                                        fieldLabel="Error Pages"
                                        name="./errorPages"
                                        rootPath="/content"/>
                                    <keywords
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Keywords"
                                        fieldLabel="Keywords"
                                        name="./keywords"/>
                                    <pageid
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="{Boolean}false"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Page ID for Analytics Tracking"
                                        fieldLabel="Analytics Page Id"
                                        name="./pageid"/>
                                    <searchable
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        defaultChecked="{Boolean}false"
                                        fieldDescription="This field is used to hide the page from stcom search results"
                                        name="./searchable"
                                        text="Exclude this page from Stcom search"
                                        value="{Boolean}true"/>
                                    <externalsearch
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        defaultChecked="{Boolean}false"
                                        fieldDescription="This field is used to hide the page from external search results"
                                        name="./externalsearch"
                                        text="Exclude this page from external search"
                                        value="{Boolean}true"/>
                                    <coachmark
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        fieldDescription="Check box to disable coach mark"
                                        name="./coachmark"
                                        text="Disable Coach Mark"
                                        value="true"/>
                                    <priority
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Set the priority for site map"
                                        fieldLabel="Priority"
                                        name="./priority"/>
                                    <attestationowner
                                        granite:id="attestation-owner-picker"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/apps/dotcom/components/workflow/userpicker/authorizable/autocomplete"
                                        emptyText="Select Attestation Owner"
                                        fieldDescription="Type or press down arrow to select the Attestation Owner"
                                        fieldLabel="Attestation Owner"
                                        forceSelection="true"
                                        id="attestation-owner-picker"
                                        name="./attestationowner"
                                        selector="user"
                                        type="string"
                                        userGroup="everyone"
                                        valueType="id"/>
                                    <attestationdate
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        emptyText="Select Attestation Date"
                                        fieldLabel="Attestation Date"
                                        name="./attestationdate"
                                        type="date"/>
                                </items>
                            </configs>
                        </items>
                    </section>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
