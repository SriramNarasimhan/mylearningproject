function check(){
try{
	var path = workflowData.getPayload().toString();
	var jcrsession = workflowSession.getSession();
	var node = jcrsession.getNode(path).getNode("jcr:content");
    if(null != node && node.hasProperty("22compliancereviewer") && node.getProperty("22compliancereviewer").getValue() != "" && node.getProperty("22compliancereviewer").getValue() != null){ 
		return false;
	}else{		
        return true;
    }
}catch(err) {
 log.info(err.message);
 return false;
}
}