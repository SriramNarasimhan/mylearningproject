<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceSuperType="/apps/dotcom/components/page/page/cq:dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container"
                rel="cq-siteadmin-admin-properties-tabs">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <location
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Location"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <locationname
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the location name"
                                        fieldLabel="Location Name"
                                        name="./loc_locationname"/>
                                    <address
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Enter the location address"
                                        fieldLabel="Address"
                                        name="./loc_address"/>
                                    <county
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the county"
                                        fieldLabel="County"
                                        name="./loc_county"/>
                                    <city
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the city"
                                        fieldLabel="City"
                                        name="./loc_city"/>
                                    <state
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                        allowCreate="{Boolean}false"
                                        cq-msm-lockable="cq:tags"
                                        fieldDescription="Select the state tag"
                                        fieldLabel="State"
                                        forceSelection="{Boolean}true"
                                        metaType="tags"
                                        multiple="{Boolean}false"
                                        name="./loc_state"
                                        rootPath="/etc/tags/states"/>
                                    <zipcode
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Enter the zip code"
                                        fieldLabel="Zip Code"
                                        name="./loc_zipcode"/>
                                    <phone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the phone number"
                                        fieldLabel="Phone"
                                        name="./loc_phone"/>
                                    <fax
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the fax number"
                                        fieldLabel="Fax"
                                        name="./loc_fax"/>
                                    <services
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                        allowCreate="{Boolean}false"
                                        cq-msm-lockable="cq:tags"
                                        fieldDescription="Select the service tags"
                                        fieldLabel="Services"
                                        forceSelection="{Boolean}true"
                                        metaType="tags"
                                        multiple="{Boolean}true"
                                        name="./loc_services"
                                        rootPath="/etc/tags/services"/>
                                    <messgaedescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Enter the special message"
                                        fieldLabel="Special Message"
                                        name="./loc_messagedescription"/>
                                    <longitude
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="This branch or atm location longitude value"
                                        fieldLabel="Longitude "
                                        name="./loc_longitude"/>
                                    <latitude
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="This branch or atm location latitude value"
                                        fieldLabel="Latitude "
                                        name="./loc_latitude"/>
                                </items>
                            </column>
                        </items>
                    </location>
                    <branch
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Branch"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <sectiontitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                        fieldLabel="Branch Lobby Hours"
                                        text="Branch Lobby Hours"/>
                                    <sunbranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the sunday lobby hours"
                                        fieldLabel="Sunday"
                                        name="./loc_sunbranchhours"/>
                                    <monbranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the monday lobby hours"
                                        fieldLabel="Monday"
                                        name="./loc_monbranchhours"/>
                                    <tuebranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the tuesday lobby hours"
                                        fieldLabel="Tuesday"
                                        name="./loc_tuebranchhours"/>
                                    <wedbranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the wednesday lobby hours"
                                        fieldLabel="Wednesday"
                                        name="./loc_wedbranchhours"/>
                                    <thubranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the thursday lobby hours"
                                        fieldLabel="Thursday"
                                        name="./loc_thubranchhours"/>
                                    <fribranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the friday lobby hours"
                                        fieldLabel="Friday"
                                        name="./loc_fribranchhours"/>
                                    <satbranchhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the saturday lobby hours"
                                        fieldLabel="Saturday "
                                        name="./loc_satbranchhours"/>
                                </items>
                            </column>
                        </items>
                    </branch>
                    <drivethru
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Drive Thru"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <sectiontitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                        fieldLabel="Drive -Thru Lobby Hours"
                                        text="Drive -Thru Lobby Hours"/>
                                    <sundriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the sunday lobby hours"
                                        fieldLabel="Sunday"
                                        name="./loc_sundriveinhours"/>
                                    <mondriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the monday lobby hours"
                                        fieldLabel="Monday"
                                        name="./loc_mondriveinhours"/>
                                    <tuedriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the tuesday lobby hours"
                                        fieldLabel="Tuesday"
                                        name="./loc_tuedriveinhours"/>
                                    <weddriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the wednesday lobby hours"
                                        fieldLabel="Wednesday"
                                        name="./loc_weddriveinhours"/>
                                    <thudriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the thursday lobby hours"
                                        fieldLabel="Thursday"
                                        name="./loc_thudriveinhours"/>
                                    <fridriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the friday lobby hours"
                                        fieldLabel="Friday"
                                        name="./loc_fridriveinhours"/>
                                    <satdriveinhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the saturday lobby hours"
                                        fieldLabel="Saturday "
                                        name="./loc_satdriveinhours"/>
                                </items>
                            </column>
                        </items>
                    </drivethru>
                    <tellerconnect
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Teller Connect"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <sectiontitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                        fieldLabel="Teller Connect Lobby Hours"
                                        text="Teller Connect Lobby Hours"/>
                                    <suntellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the sunday lobby hours"
                                        fieldLabel="Sunday"
                                        name="./loc_suntellerhours"/>
                                    <montellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the monday lobby hours"
                                        fieldLabel="Monday"
                                        name="./loc_montellerhours"/>
                                    <tuetellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the tuesday lobby hours"
                                        fieldLabel="Tuesday"
                                        name="./loc_tuetellerhours"/>
                                    <wedtellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the wednesday lobby hours"
                                        fieldLabel="Wednesday"
                                        name="./loc_wedtellerhours"/>
                                    <thutellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the thursday lobby hours"
                                        fieldLabel="Thursday"
                                        name="./loc_thutellerhours"/>
                                    <fritellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the friday lobby hours"
                                        fieldLabel="Friday"
                                        name="./loc_fritellerhours"/>
                                    <sattellerhours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the saturday lobby hours"
                                        fieldLabel="Saturday "
                                        name="./loc_sattellerhours"/>
                                </items>
                            </column>
                        </items>
                    </tellerconnect>
                    <dailyjob
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Daily Job"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <batchexecutiondate
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Update the batch execution date to manual run of location update(Example:10/24/2017)"
                                        fieldLabel="Batch Execution Date (MM/DD/YYYY)"
                                        name="./batchExecutionDate"/>
                                </items>
                            </column>
                        </items>
                    </dailyjob>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
