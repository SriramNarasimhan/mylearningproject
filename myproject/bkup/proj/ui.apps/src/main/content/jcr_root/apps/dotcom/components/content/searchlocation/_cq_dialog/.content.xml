            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <show-more-cta
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Sets the CTA label for &quot;Show More Results&quot; CTA."
                                        fieldLabel="Show More CTA*"
                                        name="./showMoreCta"
                                        required="{Boolean}true"/>
                                    <page-size
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/foundation/form/numberfield"
                                        fieldDescription="Set the default page size for the results"
                                        fieldLabel="Page Size*"
                                        min="{Long}0"
                                        name="./pagesize"
                                        required="{Boolean}true"/>
                                    <select-all-text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Sets the select all link label which is used to select all filter options"
                                        fieldLabel="Select All Text*"
                                        name="./selectAllText"
                                        required="{Boolean}true"/>
                                    <select-none-text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Sets the select none link label which is used to de-select all filter options"
                                        fieldLabel="Select None Text*"
                                        name="./selectNoneText"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </general>
                    <all-results
                        jcr:primaryType="nt:unstructured"
                        jcr:title="All Results"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
