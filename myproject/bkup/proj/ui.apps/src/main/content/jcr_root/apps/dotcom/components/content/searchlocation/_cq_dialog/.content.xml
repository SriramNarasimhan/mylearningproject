                                        name="./locationLeftRailText"
                                        required="{Boolean}true"/>
                                    <no-result
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        fieldDescription="Set the descriptive text during no match scenario"
                                        fieldLabel="No Result Text During Search*"
                                        name="./locationNoResultText"
                                        required="{Boolean}true"/>
                                    <no-result-during-filter
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        fieldDescription="Set the descriptive text during no match scenario while doing filtering"
                                        fieldLabel="No Result Text During Filter*"
                                        name="./locationNoResultTextForFilter"
                                        required="{Boolean}true"/>
                                    <help-center-label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Sets the help center link during no result scenario"
                                        fieldLabel="Help Center Label*"
                                        name="./locationHelpCenterLabel"
                                        required="{Boolean}true"/>
                                    <help-center-url
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        allowBlank="{Boolean}false"
                                        fieldDescription="Sets the help center link during no result scenario"
                                        fieldLabel="Help Center URL*"
                                        name="./locationLinkURL"
                                        required="{Boolean}true"
                                        rootPath="/content"/>
                                </items>
                            </column>
                        </items>
                    </location>
