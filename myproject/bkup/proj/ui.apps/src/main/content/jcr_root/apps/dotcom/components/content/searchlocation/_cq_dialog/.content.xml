                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Sets the Navbar label which is used to navigate between the search results"
                                        fieldLabel="Navbar Text*"
                                        name="./allResultsNavbarText"
                                        required="{Boolean}true"/>
                                    <number-of-promo-components
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/foundation/form/numberfield"
                                        fieldDescription="Sets the number of promo components allowed in parsys."
                                        fieldLabel="Allowed Promo Components*"
                                        min="{Long}0"
                                        name="./numberOfPromoComponentsInAllResults"
                                        required="{Boolean}true"/>
                                    <no-result
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        fieldDescription="Set the descriptive text during no match scenario"
                                        fieldLabel="No Result Text During Search*"
                                        name="./allResultsNoResultText"
                                        required="{Boolean}true"/>
                                    <help-center-label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Sets the help center link during no result scenario"
                                        fieldLabel="Help Center Label*"
                                        name="./allResultsHelpCenterLabel"
                                        required="{Boolean}true"/>
                                    <help-center-url
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        allowBlank="{Boolean}false"
                                        fieldDescription="Sets the help center link during no result scenario"
                                        fieldLabel="Help Center URL*"
                                        name="./allResultsLinkURL"
                                        required="{Boolean}true"
                                        rootPath="/content"/>
                                </items>
                            </column>
                        </items>
                    </all-results>
                    <location
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Location"
