function check(){
try{
	var path = workflowData.getPayload().toString();
	var jcrsession = workflowSession.getSession();
	var node = jcrsession.getNode(path).getNode("jcr:content");
    if(null != node && node.hasProperty("skiplegalreview")){
	workflowData.getMetaDataMap().put("skiplegalreview", node.getProperty("skiplegalreview").getValue());
		return true;
	}else{		
        return false;
    }
}catch(err) {
 log.info(err.message);
 return false;
}
}