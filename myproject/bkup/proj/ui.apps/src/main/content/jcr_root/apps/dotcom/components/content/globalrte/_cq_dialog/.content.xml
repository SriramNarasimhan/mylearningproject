<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Sign On"
          sling:resourceType="cq/gui/components/authoring/dialog"
          helpPath="https://www.adobe.com/go/aem6_2_docs_component_en#Text &amp; Image - Sightly">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                            name="./globalrte"
                            useFixedInlineToolbar="{Boolean}true">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <format
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <justify
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <lists
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <paraformat
                                    jcr:primaryType="nt:unstructured"
                                    features="*">
                                    <formats 
                                    	jcr:primaryType="cq:WidgetCollection">
                                    	<paragraph
                                    		jcr:primaryType="nt:unstructured"
                                            tag="p"
                                            description="Paragraph"/>
                                    	<h1 
                                    		jcr:primaryType="nt:unstructured"
                                            tag="h1"
                                            description="Heading 1"/>
                                    	<h2 
                                    		jcr:primaryType="nt:unstructured"
                                            tag="h2"
                                            description="Heading 2"/>
                                    	<h3 
                                    		jcr:primaryType="nt:unstructured"
                                            tag="h3"
                                            description="Heading 3"/>
                                    	<h4 
                                    		jcr:primaryType="nt:unstructured"
                                            tag="h4"
                                            description="Heading 4"/>
                                        <h5
                                    		jcr:primaryType="nt:unstructured"
                                            tag="h5"
                                            description="Heading 5"/>
                                        <h6
                                    		jcr:primaryType="nt:unstructured"
                                            tag="h6"
                                            description="Heading 6"/>
                                    </formats>
                            </paraformat>
                            <links
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <misctools
                                    jcr:primaryType="nt:unstructured"
                                    features="*">
                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                    <chars jcr:primaryType="nt:unstructured">
                                        <ampersand
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#38;"/>
                                        <endash
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#150;"/>
                                        <emdash
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#151;"/>
                                        <copyright
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#169;"/>
                                        <registeredtrademark
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#174;"/>
                                        <servicemark
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#8480;"/>
                                        <trademark
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#8482;"/>
                                        <centsign
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#162;"/>
                                        <pound
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#163;"/>
                                        <currencysign
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#164;"/>
                                        <yensign
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#165;"/>
                                        <euro
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#8364;"/>
                                        <left-pointingdoubleanglequotationmark
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#171;"/>
                                        <rightanglequote
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#187;"/>
                                        <dagger
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#8224;"/>
                                        <doubledagger
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#8225;"/>
                                        <feminineordinalindicator
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#170;"/>
                                        <registration
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;#174;"/>
                                    </chars>
                                </specialCharsConfig>
                            </misctools>
                            <fullscreen
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <edit
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <findreplace
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <spellcheck
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <subsuperscript
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <styles
                                    jcr:primaryType="nt:unstructured"
                                    features="*">
                                    <styles jcr:primaryType="cq:WidgetCollection">
		                                <catbtn
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="suntrust-promo-cta-button"
		                                        text="Promo CTA Button"/>
		                                <catbtnwhite
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="suntrust-promo-cta-button-white"
		                                        text="Promo CTA Button White"/>
		                                <catbtnblue
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="suntrust-promo-cta-button-blue"
		                                        text="Promo CTA Button Blue"/>
		                                <secbtn
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="suntrust-promo-cta-button-blue"
		                                        text="Secondary Button"/>
		                                 <cardcopy
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="card-copy strong.highlight"
		                                        text="Card Copy Strong Highlight"/>
		                                 <ctalink
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="suntrust-openanaccount-CTA-link"
		                                        text="Open AC CTA Link"/>
		                                  <formheading
		                                        jcr:primaryType="nt:unstructured"
		                                        cssName="suntrust-form-heading suntrust-lookup-form-heading"
		                                        text="Form Heading"/>
		                            </styles>
                            </styles>
                            <table
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                        </rtePlugins>
                        <uiSettings jcr:primaryType="nt:unstructured">
                            <cui jcr:primaryType="nt:unstructured">
                                <inline
                                        jcr:primaryType="nt:unstructured"
                                        toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,misctools#specialchars,-,findreplace#find,findreplace#replace,-,edit#cut,edit#copy,edit#paste-default,edit#paste-plaintext,edit#paste-wordhtml,-,spellcheck#checktext,-,#paraformat,-,#styles,-,table#table,table#removetable,table#insertrow,table#removerow,table#insertcolumn,table#removecolumn,table#splitcell,table#selectrow,table#selectcolumns,-]">
                                    <popovers jcr:primaryType="nt:unstructured">
                                        <justify
                                                jcr:primaryType="nt:unstructured"
                                                items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                ref="justify"/>
                                        <lists
                                                jcr:primaryType="nt:unstructured"
                                                items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                ref="lists"/>
                                        <paraformat
                                                jcr:primaryType="nt:unstructured"
                                                items="paraformat:getFormats:paraformat-pulldown"
                                                ref="paraformat"/>
                                        <styles
                                                jcr:primaryType="nt:unstructured"
                                                items="styles:getStyles:styles-pulldown"
                                                ref="styles"/>
                                    </popovers>
                                </inline>
                                 <dialogFullScreen
                                        jcr:primaryType="nt:unstructured"
                                        toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,misctools#specialchars,-,findreplace#find,findreplace#replace,-,edit#cut,edit#copy,edit#paste-default,edit#paste-plaintext,edit#paste-wordhtml,-,spellcheck#checktext,-,#paraformat,-,#styles,-,table#table,table#removetable,table#insertrow,table#removerow,table#insertcolumn,table#removecolumn,table#splitcell,table#selectrow,table#selectcolumns,-]">
                                    <popovers jcr:primaryType="nt:unstructured">
                                        <justify
                                                jcr:primaryType="nt:unstructured"
                                                items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                ref="justify"/>
                                        <lists
                                                jcr:primaryType="nt:unstructured"
                                                items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                ref="lists"/>
                                        <paraformat
                                                jcr:primaryType="nt:unstructured"
                                                items="paraformat:getFormats:paraformat-pulldown"
                                                ref="paraformat"/>
                                        <styles
                                                jcr:primaryType="nt:unstructured"
                                                items="styles:getStyles:styles-pulldown"
                                                ref="styles"/>
                                    </popovers>
                                </dialogFullScreen>
                            </cui>
                        </uiSettings>
                        <htmlRules jcr:primaryType="nt:unstructured">
                            <serializer jcr:primaryType="nt:unstructured">
                                <config
                                        jcr:primaryType="nt:unstructured"
                                        idAttribMode="keep"/>
                            </serializer>
							<docType jcr:primaryType="nt:unstructured">
                                <typeConfig
                                    jcr:primaryType="nt:unstructured"
                                    useSemanticMarkup="{Boolean}true"/>
                            </docType>
                        </htmlRules>
                    </text>
                </items>
            </column>
        </items>
    </content>
</jcr:root>

