function check(){
    try{
		var count = workflowData.getMetaDataMap().get("pageCount");
		log.info("People Update count check: "+count);
		if(count != null && count > 0){
			return false;
		} else {
			workflowData.getMetaDataMap().put("terminated", "yes");
			return true;
		}
    }catch(err) {
        log.info("People Update count check error: "+err.message);
        workflowData.getMetaDataMap().put("terminated", "yes");
			return true;
    }
}