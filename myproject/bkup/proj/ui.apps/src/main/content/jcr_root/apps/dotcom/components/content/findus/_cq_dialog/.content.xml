<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Find Us Component Dialog."
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem6_2_docs_component_en#List - Sightly">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <content
                jcr:primaryType="nt:unstructured"
                jcr:title="Content"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Sets the heading displayed in the component"
                                fieldLabel="Heading"
                                name="./heading"
                                value="Find a Branch or ATM"/>
                            <searchinput
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Sets the placeholder /empty text displayed in the search input"
                                fieldLabel="Placeholder / Empty Text"
                                name="./searchinput"
                                required="{Boolean}true"
                                value="City, State, or Zip"/>
                            <context-path
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Sets the  search results page URL"
                                fieldLabel="Search Result Page URL*"
                                name="./contextPath"
                                required="{Boolean}true"
                                rootPath="/content"/>
                            <suggestioncount
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/foundation/form/numberfield"
                                defaultValue="{Long}5"
                                fieldDescription="Suggestion count for the typeahead"
                                fieldLabel="Suggestion Count"
                                min="{Long}1"
                                name="./suggestioncount"/>
                            <dictionarysearchpath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Dictionary Lookup Path from DAM file"
                                fieldLabel="Dictionary Lookup Path"
                                name="./dictpath"
                                rootPath="/content/dam"/>
                        </items>
                    </column>
                </items>
            </content>
        </items>
    </content>
</jcr:root>
