<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Mortgage Rates"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem6_2_docs_component_en#Text &amp; Image - Sightly">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <primaryinputs
                jcr:primaryType="nt:unstructured"
                jcr:title="Primary Inputs"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <productname
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter Product title"
                                fieldLabel="Product Name"
                                name="./productname"
                                required="{Boolean}true"/>
                            <ratevalue
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter Rate (e.g. 3.990%)"
                                fieldLabel="Rate Value"
                                name="./ratevalue"
                                required="{Boolean}true"/>
                            <aprvalue
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter Rate (e.g. 3.990%)"
                                fieldLabel="APR Value"
                                name="./aprvalue"
                                required="{Boolean}true"/>
                            <originationfee
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Percentage or Amount of Fee (e.g. 0.1%)"
                                fieldLabel="Origination Fee"
                                name="./originationfee"
                                required="{Boolean}true"/>
                            <points
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Percentage or Amount of Fee (e.g. 0.1%)"
                                fieldLabel="Points"
                                name="./points"
                                required="{Boolean}true"/>
                            <ratesexamples
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                class="full-width"
                                fieldDescription="Rates Examples"
                                fieldLabel="Rates Examples">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="JSON_STORE"
                                    name="./ratesexamples">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <amount
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Enter value for Amount"
                                                    fieldLabel="Amount"
                                                    name="./amount"
                                                    required="{Boolean}true"/>
                                                <rates
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Leave empty to inherit from Rate Value field. Enter value to override the Rate Vaule."
                                                    fieldLabel="Rates"
                                                    name="./rates"/>
                                                <months
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Enter value for Months"
                                                    fieldLabel="Months"
                                                    name="./months"
                                                    required="{Boolean}true"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </ratesexamples>
                            <hideproduct
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                fieldDescription="Leave box unchecked to display this product. Check to temporarily hide product from displaying in Publisher"
                                fieldLabel="Temporarily Hide Product?"
                                name="./hideproduct"
                                text="Temporarily Hide Product?"
                                uncheckedValue="false"
                                value="true"/>
                        </items>
                    </column>
                </items>
            </primaryinputs>
            <productinfo
                jcr:primaryType="nt:unstructured"
                jcr:title="Product Info"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <anchorname
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Sets Anchor point for this Product for navigational purposes&#xa;"
                                fieldLabel="Anchor Name"
                                name="./anchorname"/>
                            <productpageurl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select Path to hyperlink Product Name"
                                fieldLabel="Product Page URL"
                                name="./productpageurl"
                                rootPath="/content"/>
                            <urlParams
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Url Params provided should be concatenated onto the url e.g. /path/to/url?param1=value1&amp;param2=value2"
                                fieldLabel="URL Parameters"
                                name="./urlParams"/>
                            <target
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                class="cq-dialog-dropdown-showhide"
                                cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                fieldDescription="Sets the window for target navigation"
                                fieldLabel="Target"
                                name="./target">
                                <items jcr:primaryType="nt:unstructured">
                                    <self
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="_self"/>
                                    <blank
                                        jcr:primaryType="nt:unstructured"
                                        text="Blank"
                                        value="_blank"/>
                                </items>
                            </target>
                            <anchorTag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Sets an anchor tag that’s applied to the url e.g. /path/to/url#tag"
                                fieldLabel="Anchor"
                                name="./anchor"/>
                            <titletag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the title tag on hover (max 100 chars)"
                                fieldLabel="Title Tag"
                                maxlength="{Long}100"
                                name="./titletag"/>
                        </items>
                    </column>
                </items>
            </productinfo>
            <rates
                jcr:primaryType="nt:unstructured"
                jcr:title="Rates"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <rateheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the text for rate type (e.g. Interest Rate or APR)"
                                fieldLabel="Rate Heading"
                                name="./rateheading"
                                required="{Boolean}true"/>
                            <aprheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the text for rate type (e.g. Interest Rate or APR)"
                                fieldLabel="APR Heading"
                                name="./aprheading"
                                required="{Boolean}true"/>
                            <originationtext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the text for fee type (e.g. Origination Fee)"
                                fieldLabel="Origination Text"
                                name="./originationtext"
                                required="{Boolean}true"/>
                            <originationurl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select Path to hyperlink Origination Text"
                                fieldLabel="Origination URL"
                                name="./originationurl"
                                rootPath="/content"/>
                            <originationurlParams
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Url Params provided should be concatenated onto the url e.g. /path/to/url?param1=value1&amp;param2=value2"
                                fieldLabel="Origination URL Parameters"
                                name="./originationurlParams"/>
                            <originationtarget
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                class="cq-dialog-dropdown-showhide"
                                cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                fieldDescription="Sets the window for target navigation"
                                fieldLabel="Target"
                                name="./originationtarget">
                                <items jcr:primaryType="nt:unstructured">
                                    <modalsmall
                                        jcr:primaryType="nt:unstructured"
                                        text="Small Modal"
                                        value="_smallmodal"/>
                                    <modalmedium
                                        jcr:primaryType="nt:unstructured"
                                        text="Medium Modal"
                                        value="_mediummodal"/>
                                    <modallarge
                                        jcr:primaryType="nt:unstructured"
                                        text="Large Modal"
                                        value="_largemodal"/>
                                    <self
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="_self"/>
                                    <blank
                                        jcr:primaryType="nt:unstructured"
                                        text="Blank"
                                        value="_blank"/>
                                </items>
                            </originationtarget>
                            <originationanchorTag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Sets an anchor tag that’s applied to the url e.g. /path/to/url#tag"
                                fieldLabel="Anchor"
                                name="./originationanchor"/>
                            <originationtitletag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the title tag on hover (max 100 chars)"
                                fieldLabel="Title Tag"
                                maxlength="{Long}100"
                                name="./originationtitletag"/>
                            <pointstext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the text for fee type (e.g. Points)"
                                fieldLabel="Points Text"
                                name="./pointstext"
                                required="{Boolean}true"/>
                            <pointsurl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select Path to hyperlink Points Text"
                                fieldLabel="Points URL"
                                name="./pointsurl"
                                rootPath="/content"/>
                            <pointsurlParams
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Url Params provided should be concatenated onto the url e.g. /path/to/url?param1=value1&amp;param2=value2"
                                fieldLabel="Points URL Parameters"
                                name="./pointsurlParams"/>
                            <pointstarget
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                class="cq-dialog-dropdown-showhide"
                                cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                fieldDescription="Sets the window for target navigation"
                                fieldLabel="Target"
                                name="./pointstarget">
                                <items jcr:primaryType="nt:unstructured">
                                    <modalsmall
                                        jcr:primaryType="nt:unstructured"
                                        text="Small Modal"
                                        value="_smallmodal"/>
                                    <modalmedium
                                        jcr:primaryType="nt:unstructured"
                                        text="Medium Modal"
                                        value="_mediummodal"/>
                                    <modallarge
                                        jcr:primaryType="nt:unstructured"
                                        text="Large Modal"
                                        value="_largemodal"/>
                                    <self
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="_self"/>
                                    <blank
                                        jcr:primaryType="nt:unstructured"
                                        text="Blank"
                                        value="_blank"/>
                                </items>
                            </pointstarget>
                            <pointsanchorTag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Sets an anchor tag that’s applied to the url e.g. /path/to/url#tag"
                                fieldLabel="Anchor"
                                name="./pointsanchor"/>
                            <pointstitletag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter the title tag on hover (max 100 chars)"
                                fieldLabel="Title Tag"
                                maxlength="{Long}100"
                                name="./pointstitletag"/>
                        </items>
                    </column>
                </items>
            </rates>
            <examples
                jcr:primaryType="nt:unstructured"
                jcr:title="Examples"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <exampletext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldDescription="Enter text to describe rates example"
                                fieldLabel="Example Text"
                                name="./exampletext"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/dotcom/components/content/globalrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                <uiSettings
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/dotcom/components/content/globalrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                <htmlRules
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/dotcom/components/content/globalrte/cq:dialog/content/items/column/items/text/htmlRules"/>
                            </exampletext>
                            <tabletitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter text for Example table title (e.g. Estimated Monthly Payment)"
                                fieldLabel="Table Title"
                                name="./tabletitle"/>
                            <thamounttext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter table heading value for first column (e.g. Amount)"
                                fieldLabel="Table Heading Amount Text"
                                name="./thamounttext"/>
                            <thratetext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter table heading value for second column (e.g. Rate)"
                                fieldLabel="Table Heading Rate Text"
                                name="./thratetext"/>
                            <thmonthstext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter table heading value for third column (e.g. Months)"
                                fieldLabel="Table Heading Months Text"
                                name="./thmonthstext"/>
                            <tablecontentalignment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Set the Table Content Alignment"
                                fieldLabel="Table Content Alignment"
                                name="./tablealignment">
                                <items jcr:primaryType="nt:unstructured">
                                    <left
                                        jcr:primaryType="nt:unstructured"
                                        text="Left"
                                        value="suntrust-alignLeft"/>
                                    <center
                                        jcr:primaryType="nt:unstructured"
                                        text="Center"
                                        value="suntrust-alignCenter"/>
                                    <right
                                        jcr:primaryType="nt:unstructured"
                                        text="Right"
                                        value="suntrust-alignRight"/>
                                </items>
                            </tablecontentalignment>
                        </items>
                    </column>
                </items>
            </examples>
            <disclaimer
                jcr:primaryType="nt:unstructured"
                jcr:title="Disclaimer"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <disclaimertext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldDescription="Enter Disclaimer Text"
                                fieldLabel="Disclaimer Text"
                                name="./disclaimertext"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/dotcom/components/content/globalrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                <uiSettings
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/dotcom/components/content/globalrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                <htmlRules
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/dotcom/components/content/globalrte/cq:dialog/content/items/column/items/text/htmlRules"/>
                            </disclaimertext>
                        </items>
                    </column>
                </items>
            </disclaimer>
        </items>
    </content>
</jcr:root>
