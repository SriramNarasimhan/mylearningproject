function check(){
	var payloadPath = workflowData.getPayload().toString();
	var jcrSession = workflowSession.getSession();
	var jcrContentNode = jcrSession.getNode(payloadPath).getNode("jcr:content");
		if(jcrContentNode.getProperty("rpx.status").getValue()=='success'){	
			return true;
		}else{	
			return false;
		}
}